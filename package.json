{
  "name": "component-document-portal",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "gen:doc-page-configs": "ts-node --project libs/compiler/tsconfig.lib.json -r dotenv/config libs/compiler/src/lib/main.ts",
    "gen:doc-page-configs:watch": "ts-node --project libs/compiler/tsconfig.lib.json -r dotenv/config libs/compiler/src/lib/main.ts --watch",
    "gen:doc-page-configs:silent": "ts-node --project libs/compiler/tsconfig.lib.json -r dotenv/config libs/compiler/src/lib/main.ts --silent",
    "gen:doc-page-configs:watch:silent": "ts-node --project libs/compiler/tsconfig.lib.json -r dotenv/config libs/compiler/src/lib/main.ts --watch --silent",
    "gen:perf-testing": "ts-node --project libs/perf/tsconfig.lib.json -r dotenv/config libs/perf/src/lib/generate-doc-pages.ts",
    "serve:cdp": "nx serve component-document-portal",
    "start:cdp": "npm-run-all -p -r gen:doc-page-configs:watch:silent serve:cdp",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx run-many --target=lint --all",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "check": "run-s format:check lint test",
    "affected:lint": "nx affected:lint --base=origin/main",
    "affected:test": "nx affected:test --parallel --base=origin/main",
    "affected:check": "run-s format:check affected:lint affected:test --base=origin/main"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@nrwl/angular": "13.8.3",
    "ngx-highlightjs": "^6.1.1",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.0",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "~13.2.0",
    "@angular/compiler-cli": "~13.2.0",
    "@angular/language-service": "~13.2.0",
    "@hirez_io/observer-spy": "2.2.0",
    "@nrwl/cli": "13.8.3",
    "@nrwl/cypress": "13.8.3",
    "@nrwl/eslint-plugin-nx": "13.8.3",
    "@nrwl/jest": "13.8.3",
    "@nrwl/linter": "13.8.3",
    "@nrwl/node": "13.8.3",
    "@nrwl/tao": "13.8.3",
    "@nrwl/workspace": "13.8.3",
    "@types/glob": "^7.2.0",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "~5.10.0",
    "@typescript-eslint/parser": "~5.10.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "cypress": "^9.1.0",
    "date-fns": "2.28.0",
    "eslint": "~8.7.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "glob-promise": "4.2.2",
    "jest": "27.2.3",
    "jest-preset-angular": "11.1.1",
    "module-from-string": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "ts-jest": "27.0.5",
    "ts-node": "^10.5.0",
    "typescript": "~4.5.2",
    "uuid": "8.3.2",
    "yargs": "^17.3.1"
  }
}
